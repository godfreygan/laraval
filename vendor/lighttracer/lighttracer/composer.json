{
  "name": "lighttracer/lighttracer",
  "description": "lightweight tracer tool support export to zipkin, cat , es etc.",
  "keywords": ["trace", "lightweight"],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "LightTracer\\": "src"
    },
    "files": [
      "src/helper.php",
      "src/flakeid.php"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  }
}
